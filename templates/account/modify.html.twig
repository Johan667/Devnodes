{% extends 'base.html.twig' %}

{% block title %}Modifier mes informations | Devnodes{% endblock %}


{% block body %}
<!-- En tête titre -->
<h1 class="text-center mt-3">Mes informations</h1>
<p class="text-center"> Ici vous pouvez modifier toutes les informations relatives à votre compte</p>
<br>


<!-- Wrapper -->
<div id="myForm">
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit">Modifier</button>
        {{ form_end(form) }}
</div>
{% endblock %}